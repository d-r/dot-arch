# Install a package
alias in = paru -S

# Uninstall a package
alias un = paru -Rs

# Update outdated packages
alias up = paru

# List outdated packages
alias out = paru -Qu

# Sync package database
alias syn = paru -Fy

# Reboot the system without going back to BIOS
alias soft-reboot = systemctl soft-reboot
